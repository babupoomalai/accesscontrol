package accesscontrol.models;

import java.util.ArrayList;
import java.util.List;

public class Tree<T> {

	private Node<T> root;

	public Tree(T node) {
		root = new Node<T>();
		root.value = node;
		root.children = new ArrayList<Tree<T>.Node<T>>();
	}

	public class Node<T> {
		private T value;
		private Node<T> parents;
		private List<Node<T>> children;
	}

}
