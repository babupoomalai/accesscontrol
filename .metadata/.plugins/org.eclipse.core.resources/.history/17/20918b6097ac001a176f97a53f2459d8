package accesscontrol.models;

import java.util.ArrayList;
import java.util.Map;

public class Tree<T> {

	private Node<T> root;

	public Tree(T data) {
		root = new Node<T>();
		root.value = data;
		root.children = new ArrayList<Node<T>>();
	}

	public class Node<T> {
		private T value;
		private Node<T> parents;
		private Map<T, Node<T>> children;
	}

}
